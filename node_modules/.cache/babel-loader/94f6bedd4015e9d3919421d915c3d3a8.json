rmData).then(response => {\r\n          this.open = false;\r\n          this.options.img = process.env.VUE_APP_BASE_API + response.imgUrl;\r\n          store.commit('SET_AVATAR', this.options.img);\r\n          this.msgSuccess(\"修改成功\");\r\n          this.visible = false;\r\n        });\r\n      });\r\n    },\r\n    // 实时预览\r\n    realTime(data) {\r\n      this.previews = data;\r\n    }\r\n  }\r\n};\r\n"]}]}